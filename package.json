{
  "name": "sass-embedded-cli",
  "version": "1.0.0-beta.0",
  "description": "Command-line interface for sass-embedded",
  "bin": {
    "node-sass": "./bin/node-sass"
  },
  "main": "./lib/index.js",
  "scripts": {
    "copy": "find node-sass/{bin,lib} -not -name index.js -type f -exec sh -c \"echo {} | sed 's/node-sass\\///' | xargs -n 1 -I {} cp node-sass/{} {}\" \\;",
    "prepare": "npm run copy",
    "pretest": "bin/node-sass test/fixtures/style.scss -o test/fixtures/actual --output-style compressed",
    "test": "cmp test/fixtures/actual/style.css test/fixtures/style.css"
  },
  "license": "MIT",
  "author": {
    "name": "Andrew Duthie",
    "email": "andrew@andrewduthie.com",
    "url": "https://andrewduthie.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aduth/sass-embedded-cli.git"
  },
  "homepage": "https://github.com/aduth/sass-embedded-cli",
  "peerDependencies": {
    "sass-embedded": "^1.0.0"
  },
  "devDependencies": {
    "sass-embedded": "^1.49.9"
  },
  "files": [
    "bin/node-sass",
    "lib"
  ],
  "dependencies": {
    "async-foreach": "^0.1.3",
    "chalk": "^4.1.2",
    "cross-spawn": "^7.0.3",
    "gaze": "^1.0.0",
    "get-stdin": "^4.0.1",
    "glob": "^7.0.3",
    "lodash": "^4.17.15",
    "meow": "^9.0.0",
    "nan": "^2.13.2",
    "sass-graph": "4.0.0",
    "stdout-stream": "^1.4.0",
    "true-case-path": "^2.2.1"
  }
}
